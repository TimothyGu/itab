<%args>
	$tourn
	$account
	$req_id 
	$school_id => undef
</%args>
<%init>

	my $req = Tab::Housing->retrieve($req_id);

	if ($req->waitlist) { 
		$req->waitlist("");
	} else {
		$req->waitlist(1);
	}

	$req->update;

	my $target = "student".$req->student->id if $req->student;
	$target = "judge".$req->judge->id if $req->judge;

	$m->redirect("/register/housing_school.mhtml?school_id=".$school_id."#$target") if $school_id;
	$m->redirect("/register/housing/index.mhtml#".$req->id);

</%init>
