<%args>
	$judge_id
	$anchor => undef
</%args>
<%perl>

	my $judge = Tab::Judge->retrieve($judge_id);

	my $gender = "F";
	$gender = "M" if $judge->gender eq "F";

	$judge->gender($gender);
	$judge->update;

	$m->redirect("$Tab::url_prefix/register/housing_school.mhtml?school_id=".$judge->school->id."#judge".$judge->id);

</%perl>
