<%args>
	$tourn
	$account
	$first
	$last
	$judge_id
</%args>
<%init> 

	my @students = Tab::Student->search( first => $first, last => $last);

</%init>

<h3>Multiple students named <% $first." ".$last %></h3>

<table cellpadding="3" width="100%">

% foreach my $student (@students) { 

% my @entries = $student->entries( tournament => $tourn->id );
% next unless (@entries);
% my $school = $entries[0]->school;

<tr>
<td>From:</td><td><% $school->chapter->name %></td>
<td>Entered in:</td>
<td>
% 	foreach my $comp (@entries) { 
% 	my $event = $comp->event;

	<% $event->abbr %>

% 	}
</td>
<td>
<a href="<%$Tab::url_prefix%>/register/strike_sn_save.mas?judge_id=<%$judge_id%>&student_id=<% $student->id%>">This one</a>
</td>
</tr>
% } 

</table>
