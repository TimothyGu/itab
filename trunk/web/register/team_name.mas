<%args>
	$tourn
	$name
	$entry_id
</%args>
<%init>

	my $entry = Tab::Entry->retrieve($entry_id);

	$name =~ s/&/and/g;

	$entry->name($name);
	$entry->update;
		
	my $err = "Entryetitor name changed to $name";
	$m->redirect("$Tab::url_prefix/register/entry/edit.mhtml?err=$err&entry_id=$entry_id");
	
		
</%init>
