<%args>
	$tourn
	$entry_id
	$part
	$student_id
</%args>

<%init>
#/

	my $entry = Tab::Entry->retrieve($entry_id);
	
	if ($part eq "no") { 
	
		$entry->student($student_id);

	} else {
	
		$entry->partner($student_id);
		
	}
	
		$entry->update;
	
	my $err = "Entry code ". $entry->code ." shifted to new student";
	$m->redirect("$Tab::url_prefix/register/entry/edit.mhtml?entry_id=$entry_id&err=$err");
	

</%init>

