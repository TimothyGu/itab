<%args>
	$tourn
	$comp_id
	$part
	$student_id
</%args>

<%init>
#/

	my $comp = Tab::Comp->retrieve($comp_id);
	
	if ($part eq "no") { 
	
		$comp->student($student_id);

	} else {
	
		$comp->partner($student_id);
		
	}
	
		$comp->update;
	
	my $err = "Competitor code ". $comp->code ." shifted to new student";
	$m->redirect("$Tab::url_prefix/register/comp_edit.mhtml?comp_id=$comp_id&err=$err");
	

</%init>

