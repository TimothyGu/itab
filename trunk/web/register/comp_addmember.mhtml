<%args>
	$entry_id
	$tourn
	$circuit
	$session
	$student_id
</%args>
<%init> 

	my @existing = Tab::EntryStudent->search( student => $student_id, entry => $entry_id );

	unless (@existing) { 

		my $membership = Tab::EntryStudent->create( {
				entry => $entry_id,
				student => $student_id
		});
	}

	my $err = "Student added to team";

	$m->redirect("$Tab::url_prefix/register/entry/edit.mhtml?entry_id=$entry_id&err=$err");


</%init>
