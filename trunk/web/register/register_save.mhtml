<%args>
	$school_id
	$register
	$from => undef
</%args>
<%init>

	my $school = Tab::School->retrieve($school_id); 

	$register = 0 if $register ne "Register";
	$register = 1 if $register eq "Register";
	

	$school->registered($register);
	$school->update;
	$m->redirect($Tab::url_prefix."/register/$from?school_id=$school_id");

</%init>

