<%args>
	$round_id
</%args>
<%init>

	my $round = Tab::Round->retrieve($round_id);
	my $msg;

	if ($round->listed) { 

		$round->listed("");
		$msg = "Clearing list has been removed from the public website";

	} else { 

		$round->listed(1);
		$msg = "List of clearing competitors has been published on the public website";

	}

	$round->update;
	$m->redirect("show.mhtml?round_id=$round_id&msg=$msg");


</%init>
