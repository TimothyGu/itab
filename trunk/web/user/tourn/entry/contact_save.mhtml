<%args>
	$school_id
	$contact_name => undef
	$contact_number => undef
</%args>
<%perl>

	my $school = Tab::School->retrieve($school_id);

	$school->contact_name($contact_name);
	$school->contact_number($contact_number);
	$school->update;	

	$m->redirect("$Tab::url_prefix/user/tourn/entry/entry.mhtml?school_id=".$school->id);

</%perl>
