<%args>
	$staff_id
	$circuit_id
	$tourn_id
</%args>
<%init>

	my $access = Tab::TournAdmin->retrieve($staff_id);

	$m->redirect("tourn_edit.mhtml?tourn_id=$tourn_id&circuit_id=$circuit_id") unless $access;

	my $msg = $access->account->first." ".$access->account->last."'s access has been revoked";

	$access->delete;

	$m->redirect("tourn_edit.mhtml?tourn_id=$tourn_id&circuit_id=$circuit_id&msg=$msg");

</%init>

