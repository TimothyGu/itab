<%args>
	$staff_id
	$league
	$account
</%args>

<%init>

	my $access = Tab::ChapterAdmin->retrieve($staff_id);
	my $tourn_id = $access->tournament->id;
	my $staff_account = $access->account;

	$access->delete;

	my $err = $staff_account->first." ".$staff_account->last." has been removed from your tab staff.";
	$m->redirect("$Tab::url_prefix/league/tourn_edit.mhtml?tourn_id=$tourn_id&err=$err");

</%init>

