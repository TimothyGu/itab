<%args>
	$room
	$tourn
</%args>
<%init>

	Tab::RoomStrike->set_sql( by_room_and_tourn  => "
		select room_strike.*
		from room_strike
		where room_strike.room = ? 
		and room_strike.tourn = ? 
		order by room_strike.name
	");

	return Tab::RoomStrike->search_by_room_and_tourn($room->id, $tourn->id);

</%init>
