<%args>
	$account
</%args>
<%init>

	Tab::Panel->set_sql( by_account => "
		select distinct panel.* 
		from panel, ballot, judge, round
		where judge.account = ?
		and judge.id = ballot.judge
		and ballot.panel = panel.id
		and ballot.audit != 1
		and panel.round = round.id
		and round.online = 1
	");

	return Tab::Panel->search_by_account($account->id);
	

</%init>
