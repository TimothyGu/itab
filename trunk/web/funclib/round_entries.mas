<%args>
	$round
</%args>
<%init>

	Tab::Entry->set_sql(by_round => "
		select distinct entry.*, panel.id as panel
			from panel,ballot,entry
			where panel.round = ? 
			and panel.id = ballot.panel
			and ballot.entry = entry.id
			order by entry.code
	");

	return Tab::Entry->search_by_round($round->id);


</%init>
