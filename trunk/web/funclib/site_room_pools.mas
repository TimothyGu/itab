<%args>
	$site
	$tourn
</%args>
<%init>


	Tab::RoomPool->set_sql( by_site => "
		select distinct room_pool.*
		from room_pool, room, event
		where room.site = ?
		and room_pool.room = room.id
		and room_pool.event = event.id
		and event.tourn = ? 
		order by room.name
	");

	return Tab::RoomPool->search_by_site( $site->id, $tourn->id );


</%init>
