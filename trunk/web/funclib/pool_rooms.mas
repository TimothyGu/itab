<%args>
	$pool
</%args>
<%init>

	return unless $pool;

	Tab::Room->set_sql( by_pool => "
		select distinct room.*
		from room, room_group_room
		where room_group_room.room_group = ? 
		and room_group_room.room = room.id
		and room.inactive = 0
		order by room.name
	");

	return Tab::Room->search_by_pool($pool->id);


</%init>
