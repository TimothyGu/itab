<%args>
	$round
</%args>
<%init>

	Tab::Panel->set_sql( empties => "
		select panel.* from panel
		where panel.round = ? 
		and not exists ( 
			select entry.id
			from entry, ballot
			where ballot.panel = panel.id
			and ballot.entry = entry.id
		)");

	my @empties = Tab::Panel->search_empties($round->id);

	Tab::debuglog("Empties are ".@empties);



</%init>
