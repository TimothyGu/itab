<%args>
	$round
</%args>
<%init>

	Tab::Panel->set_sql( highest_letter => "
		select max(cast(panel.letter as signed))
		from panel
		where round = ? 
	");

	my $letter = Tab::Panel->sql_highest_letter->select_val($round->id);

	Tab::debuglog("Letter was $letter");

	$letter++;

	Tab::debuglog("Next letter is $letter");

	return $letter;

</%init>
