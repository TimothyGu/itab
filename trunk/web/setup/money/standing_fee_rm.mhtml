<%args>
	$fee_id
	$tourn
</%args>
<%init>

	my $fine = Tab::Fine->retrieve($fee_id);

	unless ($fine->tournament->id == $tourn->id) { 

		$m->print("<p>That is not a standing fine from this tournament.  Please try again</p>");
		$m->abort;
	}

	$fine->delete;
	$m->redirect("edit.mhtml");


</%init>
