<%args>
	$set_id
</%args>
<%init>

	my $set = Tab::TiebreakSet->retrieve($set_id);

	foreach my $tb ($set->tiebreaks) { 
		$tb->delete;
	}

	my $msg = "User users tiebreak attack.  It's super effective!  Set ".$set->name." destroyed ";

	$set->delete;

	$m->redirect("tiebreaks.mhtml?msg=$msg");

</%init>
