<%args>
	$staff_id
</%args>
<%perl>

	my $staff = Tab::AccountAccess->retrieve($staff_id);

	if ($staff->nosetup) { 

		$staff->nosetup(0);

	} else {

		$staff->nosetup(1);

	}

	$staff->update;

	my $msg = "Access to the setup menu changed for ".$staff->account->first." ".$staff->account->last;

	$m->redirect("access.mhtml?msg=$msg");

</%perl>
