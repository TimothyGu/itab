<%args>
	$class_id
</%args>
<%init>

	my $class = Tab::Class->retrieve($class_id);

	foreach my $event ($class->events) { 
		$event->class("");
		$event->update;
	};

	my $msg = "Double entry grouping ".$class->name." deleted";

	$class->delete();

	$m->redirect("/setup/events/double_entry.mhtml?msg=$msg");

</%init>


