<%args>
	$event_double_id
</%args>
<%init>

	my $event_double = Tab::EventDouble->retrieve($event_double_id);

	foreach my $event ($event_double->events) { 
		$event->event_double("");
		$event->update;
	};

	my $msg = "Double entry grouping ".$event_double->name." deleted";

	$event_double->delete();

	$m->redirect("/setup/events/double_entry.mhtml?msg=$msg");

</%init>


