<%args>
	$pool_id  => undef
	$round_id => undef
	$return   => undef
</%args>
<%init>

	my @existings = Tab::RoomGroupRound->search( round => $round_id, room_group => $pool_id );

	foreach my $existing (@existings) { 
		$existing->delete;
	}

	my $msg = "Round will not draw from this room pool";
	$m->redirect("pool.mhtml?msg=$msg&pool_id=$pool_id") if $return;
	$m->redirect("edit_pools.mhtml?msg=$msg#$pool_id");

</%init>
