<%args>
	$event
</%args>
<%init>

	my @entries = $event->entries;

	my $initial_code = $event->setting("code_start");

	my %taken = ();

	foreach my $entry (sort {$a->code <=> $b->code} @entries) { 
		$taken{$entry->code}++;
	}
	
	while ($taken{$initial_code}) {
		$initial_code++;
	}

	return $initial_code;


</%init>
