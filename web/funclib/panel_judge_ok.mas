<%args>
	$panel_id
	$judge_id
</%args>
<%init>
	my $panel = Tab::Panel->retrieve($panel_id);
	my $judge = Tab::Judge->retrieve($judge_id);

	return unless $judge->active;
	return if $judge->active == 0;

	my @cannot_sees = $m->comp("/funclib/judge_cannotsee.mas", judge_id => $judge->id );

	my @comps = $panel->comps;

	foreach my $comp (@comps) { 
		foreach my $cs_id (@cannot_sees) { 
			return if ($comp->id == $cs_id);
		}
	}

	return 1;

</%init>
