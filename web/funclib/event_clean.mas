<%args>
	$event
</%args>
<%init>

	
	Tab::Event->set_sql( dedupe_students => "
		describe delete entry_student.*         
			from entry_student, entry_student es2, entry    
			where entry_student.entry = es2.entry   
			and entry_student.student < es2.student  
			and entry.id = entry_student.entry      
			and entry.event = ?
	");

	Tab::Event->sql_dedupe_students->execute($event->id);

	return;

</%init>
