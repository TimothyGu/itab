<%args>
	$round
	$entry
	$type
</%args>
<%init>


    Tab::Round->set_sql( by_type_and_entry => "
        select distinct round.*
        from round, panel, ballot
        where round.event = ? 
        and round.type = ? 
        and round.id = panel.round
        and panel.id = ballot.panel
        and ballot.entry = ? 
		and round.name < ?
    ");

	return Tab::Round->search_by_type_and_entry( $round->event->id, $type, $entry->id, $round->name);


</%init>

