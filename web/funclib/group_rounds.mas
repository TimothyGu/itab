<%args>
	$group
</%args>
<%init>

	Tab::Round->set_sql( by_group => "
		select distinct round.* 
		from round, event 
		where round.event = event.id
		and event.judge_group = ? 
		order by event.name, round.name desc
	");

	return Tab::Round->search_by_group($group->id) if $group && $group->id;
	return;

</%init>
