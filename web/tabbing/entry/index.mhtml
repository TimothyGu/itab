<%args>
	$session
	$account
	$tourn
	$judge_id => undef
	$timeslot_id => undef
	$judge_code => undef
	$audit => "Rank" 
	$err => undef
	$all => undef
	$redo => undef
	$rank_err => undef
	$rank_string => undef
	$entered => undef
</%args>

%	if ($tourn->setting("ncfl") && $session->event && $session->event->type eq "congress") { 

		<&	"ncfl_congress.mas",
			err => $err,
			all => $all,
			tourn => $tourn,
			audit => $audit,
			session => $session,
			account => $account,
			judge_id => $judge_id,
			judge_code => $judge_code,
			timeslot_id => $timeslot_id,
			rank_err => $rank_err,
			rank_string => $rank_string,
			entered => $entered &>


%	} elsif ($session->event && $session->event->type eq "wudc") { 

		<&	"wudc_rank.mas",
			err => $err,
			all => $all,
			tourn => $tourn,
			audit => $audit,
			session => $session,
			account => $account,
			judge_id => $judge_id,
			judge_code => $judge_code,
			timeslot_id => $timeslot_id,
			rank_err => $rank_err,
			rank_string => $rank_string,
			entered => $entered &>

%	} elsif ($tourn->setting("audit_method") eq "visual") { 

		<&	"visual_rank.mas",
			session => $session,
			account => $account,
			tourn => $tourn,
			judge_id => $judge_id,
			judge_code => $judge_code,
			timeslot_id => $timeslot_id,
			audit => $audit,
			all => $all,
			redo => $redo,
			err => $err,
			rank_err => $rank_err,
			rank_string => $rank_string,
			entered => $entered &>

%	} elsif ($tourn->setting("audit_method") eq "none") { 

		<&	"noaudit_rank.mas",
			session => $session,
			account => $account,
			tourn => $tourn,
			judge_id => $judge_id,
			judge_code => $judge_code,
			timeslot_id => $timeslot_id,
			audit => $audit,
			all => $all,
			err => $err,
			rank_err => $rank_err,
			rank_string => $rank_string,
			entered => $entered &>

%	} else { 

		<&	"twice_rank.mas",
			session => $session,
			account => $account,
			tourn => $tourn,
			judge_id => $judge_id,
			judge_code => $judge_code,
			timeslot_id => $timeslot_id,
			audit => $audit,
			all => $all,
			err => $err,
			rank_err => $rank_err,
			rank_string => $rank_string,
			entered => $entered &>

%	}
