<%args>
	$tourn
	$event_id
	$timeslot_id
	$speaker_order
</%args>
<%init>
	
	my $event = Tab::Event->retrieve($event_id);
	my $timeslot = Tab::Timeslot->retrieve($timeslot_id);

	foreach my $comp ($event->comps_by_timeslot_and_order($timeslot, $speaker_order)) { 

		next unless $ARGS{$comp->id};
		$comp->set_question_timeslot($timeslot, $ARGS{$comp->id});

	}

	my $err = "Questions drawn saved for the round";

	$m->redirect("$Tab::url_prefix/tabbing/extemp_questions.mhtml?event_id=$event_id&timeslot_id=$timeslot_id&speaker_order=$speaker_order&err=$err");

</%init>
