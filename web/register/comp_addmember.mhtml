<%args>
	$comp_id
	$tourn
	$circuit
	$session
	$student_id
</%args>
<%init> 

	my @existing = Tab::TeamMember->search( student => $student_id, comp => $comp_id );

	unless (@existing) { 

		my $membership = Tab::TeamMember->create( {
				comp => $comp_id,
				student => $student_id
		});
	}

	my $err = "Student added to team";

	$m->redirect("$Tab::url_prefix/register/comp_edit.mhtml?comp_id=$comp_id&err=$err");


</%init>
