<%args> 
	$tourn
</%args>
<%init>

	my @schools = $tourn->schools;

	foreach my $school (@schools) { 

		my $school_name = $school->name;
		$school_name =~ s/High\ School//g;
		$school_name =~ s/HS//g;

		my $chapter = $school->chapter;
		$chapter->name($school_name);
		$school->name($school_name);
		$school->update;
		$chapter->update;

	}


	$m->redirect("$Tab::url_prefix/register/reports.mhtml");

</%init> 
