<%args>
	$comp_id
	$tourn
	$account
</%args>
<%init>

	my $comp = Tab::Entry->retrieve($comp_id); 

	$m->comp("unwaitlist_comp.mas", comp => $comp, account => $account, tourn => $tourn);

	my $msg = "Admitted ".$comp->code." off waitlist.  Coach notified";

	$m->redirect("waitlist.mhtml?event_id=".$comp->event->id."&msg=$msg");

</%init>
