<%init>

	my @schools = Tab::School->search( tournament => 26 ); 

	foreach my $school (@schools) { 

		Tab::School->set_sql( update_sucker => "
				update judge,school             
					set judge.school = school.id            
					where school.chapter = ".$school->chapter->id."  
					and school.tournament = 54
					and judge.tournament = 54 
					and judge.school = ".$school->id );

		Tab::School->sql_update_sucker->execute;

	}


</%init>


