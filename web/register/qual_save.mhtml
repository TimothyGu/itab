<%args>
	$entry_id
	$qualifier => undef
	$qualexp => undef
	$qual2 => undef
	$qual2exp => undef
</%args>
<%init>

	my $entry = Tab::Entry->retrieve($entry_id);

	$entry->qualifier($qualifier);
	$entry->qualexp($qualexp);
	$entry->qual2($qual2);
	$entry->qual2exp($qual2exp);

	$entry->update;

	$m->redirect("$Tab::url_prefix/register/entry/edit.mhtml?entry_id=$entry_id");

</%init>
