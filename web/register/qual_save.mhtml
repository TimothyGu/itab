<%args>
	$comp_id
	$qualifier => undef
	$qualexp => undef
	$qual2 => undef
	$qual2exp => undef
</%args>
<%init>

	my $comp = Tab::Comp->retrieve($comp_id);

	$comp->qualifier($qualifier);
	$comp->qualexp($qualexp);
	$comp->qual2($qual2);
	$comp->qual2exp($qual2exp);

	$comp->update;

	$m->redirect("$Tab::url_prefix/register/entry/edit.mhtml?comp_id=$comp_id");

</%init>
