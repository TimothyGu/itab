<%args> 
	$entry_id
	$account
</%args>
<%init>

	my $entry = Tab::Entry->retrieve($entry_id);

	$m->comp("/funclib/unwaitlist_entry.mas", entry => $entry, account => $account);
	my $msg = "Admitted ".$entry->code." off waitlist.  Coach notified by email.";
	$m->redirect("roster.mhtml?event_id=".$entry->event->id."&waitlist=1&msg=$msg");
		
</%init>

