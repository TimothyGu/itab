<%args>
	$circuit
	$school_id
	$session
	$tourn
	$account
</%args>

<%init>

	my $label = "Concessions";
	$label = $tourn->method->concession_name if $tourn->method->concession_name;

	my $school = Tab::School->retrieve($school_id);
	my $filename = $Tab::file_root."/tmp/concessions-".$school_id."-".$session->id;
	
	my $garbage = `rm -f $filename.*`;

	
	open (TEXOUT, ">$filename.tex");

	print TEXOUT <<"EOF";
\\documentclass[10pt]{report}
\\usepackage{fullpage}
\\usepackage{helvet}
\\usepackage{nopageno}
\\usepackage{colortbl}
\\renewcommand{\\familydefault}{\\sfdefault}
\\renewcommand{\\arraystretch}{1.5}
\\begin{document}
\\begin{center}


EOF

	close TEXOUT;

	$m->comp("/funclib/concessions_print.mas", 
			school_id => $school_id, 
			label => $label,
			filename => $filename, 
			circuit => $circuit);

	open (TEXOUT, ">>$filename.tex");
	print TEXOUT "\\end{center}\n";
	print TEXOUT "\\end{document}\n";
	close TEXOUT;

	$garbage = `cd $Tab::file_root/tmp; $Tab::latex_path $filename.tex; $Tab::dvipdfm_path $filename.dvi`;

#	`rm -f $filename.tex $filename.log $filename.dvi $filename.aux`;

	$m->redirect("$Tab::url_prefix/tmp/concessions-$school_id-".$session->id.".pdf");

</%init>

	<div id="content">

		<p><% $filename %></p>

	</div>
