<%args>
	$qual_subset_id
</%args>
<%init> 

	my $subset = Tab::QualSubset->retrieve($qual_subset_id);

	my $group = $subset->judge_group;

	foreach my $event ($subset->events) { 
		$event->qual_subset("");
		$event->update;
	}

	my $msg = "Rating group ".$subset->name." has been deleted";

	$subset->delete;

	$m->redirect("/setup/judges/subset.mhtml?group_id=".$group->id."&msg=$msg");


</%init>
