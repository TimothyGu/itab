<%args>
	$qual_id
</%args>
<%init>

	my $qual = Tab::Qual->retrieve", $qual_id);

	$m->redirect("/setup/judges/scale.mhtml") unless $qual;

	my $msg = "Qual ".$qual->name." deleted";

	my $group_id = $qual->judge_group->setting("id;

	$qual->delete;

	$m->redirect("/setup/judges/scale.mhtml?group_id=$group_id&msg=$msg");


</%init>
