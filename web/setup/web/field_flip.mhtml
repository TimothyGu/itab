<%args>
	$event_id
</%args>
<%init>

	my $event = Tab::Event->retrieve($event_id);

	if ($event->field_report == 1) {
		$event->field_report("0");
	} else { 
		$event->field_report("1");
	}

	$event->update;

	my $msg = "Field Report Status changed for ".$event->name;

	$m->redirect("/setup/web/fields.mhtml?msg=$msg");



</%init>




