<%args>
	$set_id
	$account
</%args>
<%init>

	my $set = Tab::TiebreakSet->retrieve($set_id);

	foreach my $tb ($set->tiebreaks) { 
		$tb->delete;
	}

	my $msg = $account->first." uses TIEBREAK ATTACK.<br/><br/>  It's super effective! <br/><br/> Set ".$set->name." destroyed ";

	$set->delete;

	$m->redirect("tiebreaks.mhtml?msg=$msg");

</%init>
