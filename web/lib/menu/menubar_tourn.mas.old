<%args>
	$session
</%args>
<%init>

	unless ($session && $session->account) { 
		my $msg = "You no longer have an active login session.  Please log in again";
		$m->redirect("$Tab::url_prefix/index/index.mhtml?msg=$msg");
	}

	my $account = $session->account;
	my $tourn   = $session->tournament;
	my $circuit = $session->circuit;

	my $now = DateTime->now;

	return if $session->limited;
	return if $session->entry;

	my $nosetup;

	unless ($account->site_admin || $account->id == $tourn->director->id) {

		my @chapter_admin = Tab::ChapterAdmin->search( account => $account->id, tournament => $tourn->id ); 
		
		my $err = "You do not have access to that tournament";
		$m->redirect("/user/home.mhtml?err=$err") unless @chapter_admin; 
		
		my $acc = shift @chapter_admin;
		$nosetup++ if $acc->nosetup;

	}

</%init>


