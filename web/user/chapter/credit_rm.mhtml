<%args>
	$credit_id
</%args>
<%init>

		my $credit = Tab::Coach->retrieve($credit_id);

		my $chapter_id = $credit->chapter->id;

		my $err = "Coaching credit removed for ".$credit->name;

		$credit->delete();

		$m->redirect("$Tab::url_prefix/user/chapter/settings.mhtml?chapter_id=$chapter_id?err=$err");


</%init>
