<%args>
	$account_id
</%args>
<%init>
	
	my $account = Tab::Account->retrieve($account_id);

	foreach my $login ($account->logins) { 

		$m->comp("/funclib/ldap_account.mas", login => $login);

	}

	my $msg = "LDAP Accounts Renewed";

	$m->redirect("account_edit.mhtml?account_id=".$account->id."&msg=$msg");

</%init>
