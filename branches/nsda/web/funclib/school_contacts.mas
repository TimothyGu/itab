<%args>
	$school
</%args>
<%perl>

	Tab::Account->set_sql(by_school => "
		select distinct account.*
		from account, chapter_admin, school
		where school.id = ? 
		and school.chapter = chapter_admin.chapter
		and chapter_admin.account = account.id
		order by account.last ");


	return Tab::Account->search_by_school($school->id);

</%perl>
