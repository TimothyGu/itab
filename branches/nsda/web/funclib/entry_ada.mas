<%args>
	$entry
</%args>
<%init>

	Tab::Student->set_sql(ada_by_entry => "
		select student.id 
		from student, entry_student
		where entry_student.entry = ?
		and entry_student.student = student.id
		and student.ada = 1");

	return 1 if Tab::Student->search_ada_by_entry($entry->id);

	return;


</%init>
