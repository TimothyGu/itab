<%args>
	$tourn
	$account
	$group_id
</%args>
<%init> 
#/	


	Tab::Judge->set_sql(score_judges =>	"update judge 
			set judge.score = 
			(select count(entry.id) from entry  
			where judge.school = entry.school        
			and judge.judge_group = $group_id
			group by judge.id )");

	

	Tab::Judge->sql_score_judges->execute;

	return;

			
</%init>
