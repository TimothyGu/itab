<%args>
	$round
</%args>
<%init>
	
	my $dbh = Tab::DBI->db_Main();

	my $sth = $dbh->prepare("
		select distinct entry.id, panel.room
		from entry, panel, ballot
		where panel.round = ".$round->id."
		and panel.id = ballot.panel
		and ballot.entry = entry.id
		group by entry.id
	");

	$sth->execute();

	my %rooms_hash = ();

	while( my ($entry, $room)  = $sth->fetchrow_array() ) {
		$rooms_hash{$entry} = $room;
	}

	return %rooms_hash;

</%init>
