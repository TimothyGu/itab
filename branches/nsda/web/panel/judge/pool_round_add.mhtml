<%args>
	$pool_id
	$round_id
</%args>
<%init>
	
	my $round = Tab::Round->retrieve($round_id);
	$round->pool($pool_id);
	$round->update;

	my $msg = $round->realname." will now draw from this judge pool";
	$m->redirect("pool.mhtml?pool_id=$pool_id&group_id=".$round->event->judge_group->id."&msg=$msg");


</%init>
